        <item name="android:textSize">14sp</item>
        <item name="lineHeight">16.71dp</item>
    </style>

    <style name="Fastfood.TextAppearance.BottomBar" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/inter_medium</item>
        <item name="android:textSize">12sp</item>
        <item name="lineHeight">16dp</item>
