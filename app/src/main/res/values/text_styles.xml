<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <style name="Fastfood.TextAppearance.Toolbar" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="lineHeight">18.75dp</item>
    </style>

    <style name="Fastfood.TextAppearance.Tab" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/sf_ui_display_semibold</item>
        <item name="android:textSize">13sp</item>
        <item name="lineHeight">15.51dp</item>
    </style>

    <style name="Fastfood.TextAppearance.TabNoActive" parent="TextAppearance.AppCompat">
